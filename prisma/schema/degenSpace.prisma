enum FlickType {
  ReFlick 
  PrimaryFlick
}


type FlickAnalytics {
  views Int,
  clicks Int,
  profileVisits Int,
  impressions Int
}

// a Post -> Flicks ( I flicked )
model Flick{ 
  id String @id @default(cuid())
  userId String 
  flickType FlickType @default(ReFlick)
  // if flicks are replies
  parentFlickId String?
  flickData String
  // likes
  respectCount Int
  // reposts
  cloneCount Int
  // bookmarks
  dumpCount Int

  flickAnalytics FlickAnalytics
 } 
 
// User <-> ChadEngagement
model ChadEngagement{
  id @id @default(cuid())
  flickId String
  hasLiked Boolean
  hasShared Boolean
}



